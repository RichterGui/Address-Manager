{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "eslint-plugin-import/no-extraneous-dependencies": "off",
        "class-methods-use-this": "off",
        "eslint-plugin-import/extensions": "on"
    }
}